blc {
Program = Term args
Term
  = lb lambda PairOrSingle rb -- bracketedlambda
  | lambda PairOrSingle       -- lambda
  | lb PairOrSingle rb        -- nested
  | lb rb                     -- empty
  | index                     -- index

PairOrSingle
  = Term PairOrSingle -- application
  | Term Term         -- two
  | Term              -- single
  
lb = "[" | "("
rb = "]" | ")"
lambda="Î»"
index="0" .. "9"
args=any*
}