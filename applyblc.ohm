applyblc {
Program = Term args
Term
  = lb "apply" Term Term rb -- apply
  | lb lambda Term Term rb  -- bracketedlambdacall
  | lambda Term Term        -- lambdacall
  | lambda Term             -- lambda
  | lb Term Term rb         -- call
  | lb Term rb              -- nested
  | lb rb                   -- empty
  | index                   -- index
  | string                  -- string
  
lb = "[" | "("
rb = "]" | ")"
lambda="Î»"
index="0" .. "9"
string = dq (~dq any)* dq
dq = "\""
args=any*
}