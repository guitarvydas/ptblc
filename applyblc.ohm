blcwithapply {
Program = Term args
Term
  = lb "apply" Term Term rb   -- apply
  | lb lambda PairOrSingle rb -- bracketedlambda
  | lambda Term       -- lambda
  | lb PairOrSingle rb        -- nested
  | lb rb                     -- empty
  | index                     -- index
  | string                    -- string

PairOrSingle
  = Term PairOrSingle -- application
  | Term Term         -- two
  | Term              -- single
  
lb = "[" | "("
rb = "]" | ")"
lambda="Î»"
index="0" .. "9"
args=(~rb any)*
string = dq (~dq any)* dq
dq = "\""
}

